{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:prettier/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "prettier/prettier": ["error", { "endOfLine": "auto" }]
    },
    
    "comma-dangle": ["error", "never"]
}
